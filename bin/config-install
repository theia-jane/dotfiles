#!/usr/bin/env -S emacs -Q --script # -*- mode: emacs-lisp; lexical-binding: t; -*-
;; [[file:~/Projects/dotfiles/config-cli.org::*Install][Install:1]]
(require 'org)
(require 'org-element)
(require 'ob-core)
(require 'ob-shell)

(defvar config-root (replace-regexp-in-string "bin/[^/]+$" "" load-file-name))
(setq org-confirm-babel-evaluate nil)
(dolist (file (directory-files-recursively config-root "^install\\.org$"))
  (with-temp-buffer
    (insert-file-contents file)
    (org-mode)
    (org-babel-execute-buffer)))
;; Install:1 ends here
